models/weapons2/droidblst/blas-tek
{
    {
        map models/weapons2/droidblst/blas-tek1
        rgbGen lightingDiffuse
    }
    {
        map models/weapons2/envmap1
        blendFunc GL_ONE GL_ONE
        rgbGen lightingDiffuse
        tcGen environment
    }
    {
        map models/weapons2/droidblst/blas-tek
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
}

