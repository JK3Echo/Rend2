gfx/effects/zam_blaster_blob
{
	cull	twosided
    {
        map gfx/effects/zam_blaster_blob
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		glow
        alphaGen vertex
    }
}

