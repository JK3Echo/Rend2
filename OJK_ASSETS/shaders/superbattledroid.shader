d/fm3/base/models/players/superbattledroid/body
{
	q3map_nolightmap
    {
        map models/players/superbattledroid/body
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/superbattledroid/chrome
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/superbattledroid/body_s
        alphaFunc GT0
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
}

