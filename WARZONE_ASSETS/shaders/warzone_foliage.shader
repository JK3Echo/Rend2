models/pop/foliages/alp_weedy_c
{
	qer_editorimage	models/pop/foliages/alp_weedy_c
	q3map_nolightmap
	q3map_alphashadow
	q3map_onlyvertexlighting
	q3map_material	ShortGrass
	cull	twosided
    {
        map models/pop/foliages/alp_weedy_c
        alphaFunc GE128
        rgbGen lightingDiffuse
    }
}

